#!/bin/sh

printUsage()
{
    cat <<HELPTEXT
Write arguments to the standard input, each on a separate line.
HELPTEXT
    printf 'Usage: %q %s\n' "$(basename "$1")" '[arg ...] [-?|-h|--help]'
}
case "$1" in
    --help|-h|-\?)	shift; printUsage "$0"; exit 0;;
esac

if [ $# -eq 0 ]; then
    printf ''
else
    printf '%s\n' "$@"
fi
